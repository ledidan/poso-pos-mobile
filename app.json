{
  "expo": {
    "name": "poso-note-app",
    "slug": "poso-note-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/android/ic_launcher.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poso.poso-note-app",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "splash": {
      "image": "./assets/AppIcons/android/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons/android/ic_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/AppIcons/android/ic_launcher.png"
    }
  }
}
