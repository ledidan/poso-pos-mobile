{
  "expo": {
    "name": "Poso",
    "slug": "poso-note",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/android/mipmap-hdpi/ic_launcher.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poso.poso-note",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "splash": {
      "image": "./assets/AppIcons/android/mipmap-hdpi/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons/android/mipmap-hdpi/ic_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ledidan.posonote"
    },
    "web": {
      "favicon": "./assets/AppIcons/android/mipmap-hdpi/ic_launcher.png"
    },
    "extra": {
      "eas": {
        "projectId": "660152c8-0f1e-40f0-ba41-30413ecec418"
      }
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "CUSTOM: Allow Poso to access the microphone",
          "speechRecognitionPermission": "CUSTOM: Allow Poso to securely recognize user speech"
        }
      ],
      "expo-font"
    ],
    "owner": "ledidan"
  }
}
